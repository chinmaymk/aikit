{
  "name": "@chinmaymk/aikit",
  "version": "0.0.32",
  "description": "Lightweight generation abstraction for OpenAI, Anthropic and Google Gemini",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:results": "JEST_JUNIT_CLASSNAME=\"{filepath}\" jest --reporters=jest-junit",
    "test:smoke": "jest --config tests/smoke/jest.config.js --verbose",
    "dev": "ts-node test.ts",
    "test:basic": "ts-node tests/basic.ts",
    "test:compatibility": "ts-node tests/compatibility.ts",
    "test:all": "npm run test:basic && npm run test:compatibility",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --quiet src scripts examples --ext .ts,.js",
    "lint:fix": "eslint src scripts examples --ext .ts,.js --fix",
    "typecheck": "tsc --noEmit",
    "build": "npm run lint && npm run typecheck && npm run format:check && npm run test && npm run compile",
    "compile": "tsc --build tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "docs:generate": "typedoc",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npm run docs:generate && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "openai",
    "anthropic",
    "google",
    "gemini",
    "ai",
    "llm"
  ],
  "author": "Chinmay Kulkarni",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nock": "^14.0.5",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "0.28.5",
    "typedoc-plugin-markdown": "^4.0.0",
    "typescript": "^5.8.3",
    "vitepress": "^1.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chinmaymk/aikit.git"
  },
  "bugs": {
    "url": "https://github.com/chinmaymk/aikit/issues"
  },
  "homepage": "https://chinmaymk.github.io/aikit/"
}
